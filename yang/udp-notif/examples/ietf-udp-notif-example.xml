<?xml version='1.0' encoding='UTF-8'?>
<config xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <filters xmlns="urn:ietf:params:xml:ns:yang:ietf-subscribed-notifications">
    <stream-filter>
      <name/>
      <stream-xpath-filter/>
    </stream-filter>
  </filters>
  <subscriptions xmlns="urn:ietf:params:xml:ns:yang:ietf-subscribed-notifications">
    <subscription>
      <id/>
      <stream-filter-name/>
      <stream-xpath-filter/>
      <stream/>
      <configured-replay/>
      <stop-time/>
      <dscp>0</dscp>
      <weighting/>
      <dependency/>
      <transport/>
      <encoding/>
      <purpose/>
      <source-interface/>
      <source-vrf/>
      <source-address/>
      <receivers>
        <receiver>
          <name/>
          <receiver-instance-ref xmlns="urn:ietf:params:xml:ns:yang:ietf-subscribed-notif-receivers"/>
        </receiver>
      </receivers>
    </subscription>
    <receiver-instances xmlns="urn:ietf:params:xml:ns:yang:ietf-subscribed-notif-receivers">
      <receiver-instance>
        <name/>
        <udp-notif-receiver xmlns="urn:ietf:params:xml:ns:yang:ietf-udp-notif-transport">
          <address/>
          <port/>
          <enable-segmentation>false</enable-segmentation>
          <max-segment-size/>
          <dtls>
            <client-identity>
              <certificate>
                <local-definition>
                  <public-key-format/>
                  <public-key/>
                  <private-key-format/>
                  <cleartext-private-key/>
                  <hidden-private-key/>
                  <encrypted-private-key>
                    <encrypted-by/>
                    <encrypted-value-format/>
                    <encrypted-value/>
                  </encrypted-private-key>
                  <cert-data/>
                </local-definition>
                <keystore-reference>
                  <asymmetric-key/>
                  <certificate/>
                </keystore-reference>
              </certificate>
              <raw-private-key>
                <local-definition>
                  <public-key-format/>
                  <public-key/>
                  <private-key-format/>
                  <cleartext-private-key/>
                  <hidden-private-key/>
                  <encrypted-private-key>
                    <encrypted-by/>
                    <encrypted-value-format/>
                    <encrypted-value/>
                  </encrypted-private-key>
                </local-definition>
                <keystore-reference/>
              </raw-private-key>
              <tls13-epsk>
                <local-definition>
                  <key-format/>
                  <cleartext-key/>
                  <hidden-key/>
                  <encrypted-key>
                    <encrypted-by/>
                    <encrypted-value-format/>
                    <encrypted-value/>
                  </encrypted-key>
                </local-definition>
                <keystore-reference/>
                <external-identity/>
                <hash/>
                <context/>
                <target-protocol/>
                <target-kdf/>
              </tls13-epsk>
            </client-identity>
            <server-authentication>
              <ca-certs>
                <local-definition>
                  <certificate>
                    <name/>
                    <cert-data/>
                  </certificate>
                </local-definition>
                <truststore-reference/>
              </ca-certs>
              <ee-certs>
                <local-definition>
                  <certificate>
                    <name/>
                    <cert-data/>
                  </certificate>
                </local-definition>
                <truststore-reference/>
              </ee-certs>
              <raw-public-keys>
                <local-definition>
                  <public-key>
                    <name/>
                    <public-key-format/>
                    <public-key/>
                  </public-key>
                </local-definition>
                <truststore-reference/>
              </raw-public-keys>
              <tls13-epsks/>
            </server-authentication>
            <hello-params>
              <tls-versions>
                <tls-version>
                  <!-- # entries: 0.. -->
                </tls-version>
              </tls-versions>
              <cipher-suites>
                <cipher-suite>
                  <!-- # entries: 0.. -->
                </cipher-suite>
              </cipher-suites>
            </hello-params>
            <keepalives>
              <peer-allowed-to-send/>
              <test-peer-aliveness>
                <max-wait>30</max-wait>
                <max-attempts>3</max-attempts>
              </test-peer-aliveness>
            </keepalives>
          </dtls>
        </udp-notif-receiver>
      </receiver-instance>
    </receiver-instances>
  </subscriptions>
</config>
